&gt;&gt;&gt;&gt;&gt;&gt;         class=&quot;my.plugin.name.ui.actions.PropertyActionProvider&quot;<br />
&gt;&gt;&gt;&gt;&gt;&gt;         id=&quot;my.plugin.name.ui.actions.OpenProvider&quot;&gt;<br />
