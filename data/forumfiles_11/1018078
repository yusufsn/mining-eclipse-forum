&gt;         int entityId;<br />
&gt;         int collectionTypeId; // The FK I want automatically set.<br />
&gt;         private PropertyChangeSupport changeSupport = new <br />
&gt; PropertyChangeSupport(<br />
&gt;                 this);<br />
&gt; <br />
&gt;         public void addPropertyChangeListener(PropertyChangeListener <br />
&gt; listener) {<br />
&gt;             changeSupport.addPropertyChangeListener(listener);<br />
&gt;         }<br />
&gt; <br />
&gt;         /**<br />
&gt;          * @return Returns the collectionTypeId.<br />
&gt;          */<br />
&gt;         public int getCollectionTypeId() {<br />
&gt;             return collectionTypeId;<br />
&gt;         }<br />
&gt; <br />
&gt;         /**<br />
&gt;          * @param collectionTypeId<br />
&gt;          *            The collectionTypeId to set.<br />
&gt;          */<br />
&gt;         public void setCollectionTypeId(int collectionTypeId) {<br />
&gt;             changeSupport.firePropertyChange(&quot;collectionTypeId&quot;,<br />
&gt;                     this.collectionTypeId,<br />
&gt;                     this.collectionTypeId = collectionTypeId);<br />
&gt;         }<br />
&gt;     }<br />
